<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>product_list.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>CarServlet</servlet-name>
        <servlet-class>cn.andios.servlet.CarServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CarServlet</servlet-name>
        <url-pattern>/CarServlet</url-pattern>
    </servlet-mapping><servlet>
        <servlet-name>ClearCartServlet</servlet-name>
        <servlet-class>cn.andios.servlet.ClearCartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClearCartServlet</servlet-name>
        <url-pattern>/ClearCartServlet</url-pattern>
    </servlet-mapping>
</web-app>